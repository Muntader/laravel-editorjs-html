{
    "name": "ankitech/laravel-editorjs-html",
    "type": "metapackage",
    "require": {
        "php": "^7.3|^8.0",
        "codex-team/editor.js": "^2.0",
        "illuminate/support": "^6|^7|^8"
    },
    "authors": [
        {
            "name": "Muntadher",
            "email": "dev@ankitech.co"
        }
    ],
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Ankitech\\LaravelEditorjsHtml\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ankitech\\LaravelEditorjsHtml\\EditorJSHtmlProvider"
            ],
            "aliases": {
                "EditorJSHtml": "Ankitech\\LaravelEditorjsHtml\\Facades\\EditorJSHtml"
            }
        }
    }
}